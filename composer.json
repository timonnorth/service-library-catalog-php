{
    "name": "timonnorth/service-library-catalog-php",
    "type": "service",
    "description": "Library  catalog",
    "keywords": [
        "timonnorth",
        "service-library-catalog-php"
    ],
    "homepage": "https://github.com/timonnorth/service-library-catalog-php",
    "license": "MIT",
    "authors": [
        {
            "name": ":Oleksandr Ieremeev",
            "email": "fake@email.com",
            "homepage": "https://github.com/timonnorth",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.4",
        "php-di/php-di": "^6.2",
        "ext-pdo": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit" : "^9.0",
        "squizlabs/php_codesniffer": "^3.5",
        "davedevelopment/phpmig": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "LibraryCatalog\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --coverage-clover build/logs/clover.xml",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
